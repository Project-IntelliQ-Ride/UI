<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="dashpanel" prefHeight="508.0" prefWidth="1030.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controller.ownerDashBoardController">
   <children>
      <AnchorPane fx:id="pane" prefHeight="651.0" prefWidth="284.0" style="-fx-background-color: #331D2C;">
         <children>
            <Label layoutX="64.0" layoutY="21.0" prefHeight="61.0" prefWidth="151.0" text="DashBoard" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Times New Roman Bold" size="30.0" />
               </font>
            </Label>
            <Button layoutX="6.0" layoutY="114.0" mnemonicParsing="false" onAction="#manageEmployeebtn" onMouseClicked="#emplyeeOnAction" prefHeight="61.0" prefWidth="268.0" text="Manage Employee" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </Button>
            <Button layoutX="6.0" layoutY="287.0" mnemonicParsing="false" onAction="#ManageStockOnAction" prefHeight="61.0" prefWidth="268.0" text="Manage Stock" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </Button>
            <Button layoutX="6.0" layoutY="199.0" mnemonicParsing="false" onAction="#ManageBusesOnAction" prefHeight="61.0" prefWidth="268.0" text="Manage Buses" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </Button>
            <Button layoutX="7.0" layoutY="373.0" mnemonicParsing="false" onAction="#ManageProfitOnAction" prefHeight="61.0" prefWidth="268.0" text="Manage Profit" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </Button>
            <Button layoutX="7.0" layoutY="460.0" mnemonicParsing="false" onAction="#MonitorDriverOnAction" prefHeight="61.0" prefWidth="268.0" text="Monitor Driver" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </Button>
            <Button layoutX="70.0" layoutY="571.0" mnemonicParsing="false" onAction="#btnLogOut" prefHeight="46.0" prefWidth="115.0" style="-fx-background-color: #512B81; -fx-background-radius: 20;" text="Log Out" textFill="WHITE">
               <font>
                  <Font name="Times New Roman Bold" size="21.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="root" layoutX="284.0" prefHeight="512.0" prefWidth="746.0">
         <children>
            <ImageView fx:id="dashboardimgid" fitHeight="693.0" fitWidth="935.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../assests/dashboard.png" />
               </image>
            </ImageView>
         </children></AnchorPane>
   </children>
</AnchorPane>
